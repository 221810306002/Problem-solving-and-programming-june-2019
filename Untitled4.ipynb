    "    sum = 0\n",
    "    lst = list(str)\n",
    "for x in range (len(lst)):\n",
    "    if ord(lst[x]) >=48 and ord(lst[x]) <=57:\n",
    "        ac=ord(lst[x])-48\n",
    "    if(ac%2==0):\n",
    "        sum=sum+ac\n",
    "                                \n",
    "return sum\n",
    "\n",
    "print(sumOnlyEvenDigits(\"1445\"))"
